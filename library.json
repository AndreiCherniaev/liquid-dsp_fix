{
  "name": "liquid-dsp",
  "version": "1.4.0",
  "description": "Software-defined radio digital signal processing library",
  "keywords": "digital signal processing, DSP, filter, FFT, resample, interpolate, decimate, AGC, error correction, modulation",
  "repository":
  {
    "type": "git",
    "url": "https://github.com/jgaeddert/liquid-dsp.git"
  },
  "authors":
  [
    {
      "name": "Joseph D. Gaeddert",
      "email": "joseph@liquidsdr.org",
      "url": "https://liquidsdr.org/colophon",
      "maintainer": true
    }
  ],
  "license": "MIT",
  "homepage": "https://liquidsdr.org",
  "build": {
    "flags": ["-I.", "-Wno-deprecated-declarations"],
    "srcFilter": "+<*/src/*.c> -<.git/> -<*/src/*.proto.c> -<*/src/*.av.c> -<*/src/*.mmx.c> -<*/src/*.neon.c> -<*/src/*.sse4.c> -<*/src/*.x86.s>"
  }
}
